#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

// numbers_len은 배열 numbers의 길이입니다.
int solution(int numbers[], size_t numbers_len, int n) {
    int answer = 0;
    int i = 0;
    while(1){
        if(answer > n)
           return answer;
        else{
            answer += numbers[i];
            i++;
        }
    }
}
